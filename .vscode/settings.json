{
  "sourcekit-lsp.serverPath": "/usr/bin/sourcekit-lsp",
  "sourcekit-lsp.toolchainPath": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/xcuserdata": true,
    "**/.build": true,
    "**/.swiftpm": true,
    "**/DerivedData": true
  },
  "search.exclude": {
    "**/xcuserdata": true,
    "**/.build": true,
    "**/DerivedData": true
  },
  "[swift]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.formatOnSave": true
  }
}
